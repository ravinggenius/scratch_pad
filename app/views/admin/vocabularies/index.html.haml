%ul
  - @vocabularies.each do |vocabulary|
    %li
      = link_to vocabulary.name, edit_admin_vocabulary_path(vocabulary)
      = link_to 'x', admin_vocabulary_path(vocabulary), :confirm => 'Are you sure?', :method => :delete
      %ul
        - vocabulary.terms.each do |term|
          %li
            = link_to term.name, edit_admin_vocabulary_term_path(term)
